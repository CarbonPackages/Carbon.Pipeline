    "esbuild": "^0.19.5",
    "eslint": "^8.51.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "postcss": "^8.4.31",
    "postcss-assets": "^6.0.0",
    "postcss-clip-path-polyfill": "~1.1.0",
    "postcss-import": "^15.1.0",
    "postcss-reporter": "^7.0.5",
